proj_name = rcstest

rcstest_autodepends = .AUTODEPEND

!include cproj.mif
!include defrule.mif
!include deftarg.mif

!include $(rcsdll_dir)/client.mif

#
# cflags
#
.c : ..

inc_dirs = -I.. $(rcsdll_inc_dirs)

# explicit rules
#################

#
# EXE
#
exetarg_objs = rcstest.obj
exetarg_libs = $(rcsdll_lib)
exetarg_res = rcstest.res
!include exetarg.mif

#
# rc and others
#
rc_inc_dirs = -I..

rcstest.res: ../rcstest.rc $(__MAKEFILES__) .AUTODEPEND
    @%make echo_rc
    $(rc) $(rc_flags) -ad -zk0 $(rc_inc_path) $[@ -fo=$@
