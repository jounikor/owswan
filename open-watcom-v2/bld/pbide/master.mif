proj_name = pbide
name = dlldbg

pbide_autodepends = .AUTODEPEND

!include cproj.mif
!include defrule.mif
!include deftarg.mif

#
# cflags
#
.c: ..

extra_c_flags_win =-zW

# explicit rules
#################

#
# EXE
#
exetarg_name = $(name)
exetarg_objs = dlldbg.obj
exetarg_res  = $(name).res
!include exetarg.mif

#
# rc and others
#
!include verrc.mif

$(name).res : ../dlldbg.rc ../debugger.ico $(__MAKEFILES__)
    @%make echo_rc
    $(rc) $(rc_flags) $[@ -fo=$@
