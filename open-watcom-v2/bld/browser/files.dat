[DEFAULT type="e" where="c jc" desc="Browsing information merger" ]
dir="binw"      usr="wbrg.exe"  cond="winhost os2host |"                old="binb"
dir="binnt"     usr="wbrg.exe"  cond="wnthost wnt64host |"              old="binb"
dir="axpnt"     usr="wbrg.exe"  cond="alphahost"    where="ac"
