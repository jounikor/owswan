        CONTROL "Terminate Process", INT_TERMINATE, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 26, 78, 78, 10
        CONTROL "Restart Instruction", INT_RESTART, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 27, 91, 79, 10
