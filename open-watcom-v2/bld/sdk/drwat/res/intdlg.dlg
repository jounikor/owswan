        CONTROL "0xFFFFFFFF", INT_TASK_PATH, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 62, 17, 117, 8
        CONTROL "Access Violation", INT_FAULT_TYPE, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 62, 26, 116, 8
        CONTROL "0000:00000000", INT_CS_IP, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 62, 35, 116, 8
        CONTROL "line 4 of  ", INT_SOURCE_INFO, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 62, 44, 156, 8
        CONTROL "APPLICON", 119, "STATIC", SS_ICON | WS_CHILD | WS_VISIBLE, 196, 12, 18, 20
        CONTROL "", INT_SOURCE_INFO2, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 63, 53, 156, 8
        CONTROL "&Act ", INT_ACT, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 3, 120, 47, 14
END

