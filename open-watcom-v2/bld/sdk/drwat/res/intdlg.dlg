        CONTROL "&Registers...", INT_REGISTERS, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 119, 120, 47, 14
        CONTROL "Act && &Log", INT_ACT_AND_LOG, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 56, 120, 47, 14
        CONTROL "Task Name:", INT_TASK_NAME_TEXT, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 9, 8, 53, 8
        CONTROL "Task Path:", INT_TASK_PATH_TEXT, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 9, 17, 53, 8
