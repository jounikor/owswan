        CONTROL "Horizontal Bar",   IDB_TBS_HORZ, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 95, 70, 60, 10
        CONTROL "Ticks on Bottom",  IDB_TBS_BOTTOM, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE | WS_GROUP | WS_TABSTOP, 95, 85, 65, 10
        CONTROL "Ticks on Top",     IDB_TBS_TOP, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 95, 95, 65, 10
        CONTROL "Ticks on Right",   IDB_TBS_RIGHT, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE | WS_GROUP | WS_TABSTOP, 95, 110, 65, 10
        CONTROL "Ticks on Left",    IDB_TBS_LEFT, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 95, 120, 65, 10
