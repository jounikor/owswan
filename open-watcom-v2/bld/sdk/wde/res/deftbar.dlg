        CONTROL "Ticks Both Sides", IDB_TBS_BOTH, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 10, 70, 75, 10
        CONTROL "No Ticks",         IDB_TBS_NOTICKS, "BUTTON", BS_AUTORADIOBUTTON | WS_CHILD | WS_VISIBLE, 10, 80, 75, 10
        CONTROL "Auto Ticks",       IDB_TBS_AUTOTICKS, "BUTTON", BS_AUTOCHECKBOX | WS_CHILD | WS_VISIBLE | WS_GROUP | WS_TABSTOP, 10, 95, 75, 10
        CONTROL "Enable Sel Range", IDB_TBS_ENABLESELRANGE, "BUTTON", BS_AUTOCHECKBOX | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 10, 105, 75, 10
