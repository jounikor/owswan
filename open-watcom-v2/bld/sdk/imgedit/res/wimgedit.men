        MENUITEM sp( "C&lear\tDel",
                     "C&lear\tDel" ), IMGED_CLEAR
        MENUITEM SEPARATOR
        POPUP sp( "S&hift",
                  "S&hift" )
        BEGIN
            MENUITEM sp( "&Right\tShift+Right",
                         "&Right\tShift+Right" ), IMGED_RIGHT
            MENUITEM sp( "&Left\tShift+Left",
                         "&Left\tShift+Left" ), IMGED_LEFT
            MENUITEM sp( "&Up\tShift+Up",
                         "&Up\tShift+Up" ), IMGED_UP
            MENUITEM sp( "&Down\tShift+Down",
                         "&Down\tShift+Down" ), IMGED_DOWN
