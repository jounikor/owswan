        MENUITEM sp( "Show &View Window(s)\tF8",
                     "Show &View Window(s)\tF8" ), IMGED_VIEW
    END
    POPUP sp( "&Palette",
              "&Palette" )
    BEGIN
        MENUITEM sp( "&Edit Current Color...",
                     "&Edit Current Color..." ), IMGED_CEDIT
        MENUITEM sp( "&Reset Current Palette",
                     "&Reset Current Palette" ), IMGED_CRESET
        MENUITEM sp( "&Set Screen Color...",
                     "&Set Screen Color..." ), IMGED_CSCREEN
        MENUITEM SEPARATOR
        MENUITEM sp( "&Load Color Palette...",
                     "&Load Color Palette..." ), IMGED_LCOLOR
        MENUITEM sp( "Sa&ve Color Palette...",
                     "Sa&ve Color Palette..." ), IMGED_SCOLOR
        MENUITEM sp( "Res&tore Color Palette",
                     "Res&tore Color Palette" ), IMGED_RCOLOR
    END
    POPUP sp( "&Tools",
              "&Tools" )
    BEGIN
        MENUITEM sp( "&Pencil",
                     "&Pencil" ), IMGED_FREEHAND
        MENUITEM sp( "&Line",
                     "&Line" ), IMGED_LINE
        POPUP sp( "&Ellipse",
                  "&Ellipse" )
        BEGIN
            MENUITEM sp( "&Outline",
                         "&Outline" ), IMGED_CIRCLEO
            MENUITEM sp( "&Solid",
                         "&Solid" ), IMGED_CIRCLEF
        END
        POPUP sp( "&Rectangle",
                  "&Rectangle" )
        BEGIN
            MENUITEM sp( "&Outline",
                         "&Outline" ), IMGED_RECTO
            MENUITEM sp( "&Solid",
                         "&Solid" ), IMGED_RECTF
        END
        MENUITEM sp( "&Fill",
                     "&Fill" ), IMGED_FILL
        MENUITEM sp( "&Brush",
                     "&Brush" ), IMGED_BRUSH
        MENUITEM sp( "&Select Region",
                     "&Select Region" ), IMGED_CLIP
        MENUITEM sp( "&Hot Spot",
                     "&Hot Spot" ), IMGED_HOTSPOT
    END
    POPUP sp( "&Windows",
              "&Windows" )
    BEGIN
        MENUITEM sp( "&Arrange Icons",
                     "&Arrange Icons" ), IMGED_ARRANGE
        MENUITEM sp( "&Tile Windows\tShift+F4",
                     "&Tile Windows\tShift+F4" ), IMGED_TILE
        MENUITEM sp( "&Cascade Windows\tShift+F5",
                     "&Cascade Windows\tShift+F5" ), IMGED_CASCADE
    END
    POPUP sp( "&Help",
              "&Help" )
    BEGIN
        MENUITEM sp( "&Contents",
                     "&Contents" ), IMGED_HELP
        MENUITEM sp( "&Search for Help On...",
                     "&Search for Help On..." ), IMGED_HELP_SEARCH
        MENUITEM SEPARATOR
        MENUITEM sp( "&How to Use Help",
                     "&How to Use Help" ), IMGED_HELP_ON_HELP
        MENUITEM SEPARATOR
        MENUITEM sp( "&About...",
                     "&About..." ), IMGED_ABOUT
    END
END

