"IMGEDMENU" MENU
BEGIN
    POPUP sp( "&File",
              "&File" ) 
    BEGIN
        MENUITEM sp( "&New...\tCtrl+N",
                     "&New...\tCtrl+N" ), IMGED_NEW
        MENUITEM sp( "&Open...\tCtrl+O",
                     "&Open...\tCtrl+O" ), IMGED_OPEN
