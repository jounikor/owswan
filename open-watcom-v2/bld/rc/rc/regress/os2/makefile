# wrc OS/2 support regression tests
#
# Note: The .rez files are generated by IBM's RC, hence -zm option for wrc

RC = wrc
RCFLAGS = -q -zm -r -bt=os2 -I"$(%WATCOM)/h/os2"

.SUFFIXES : .res .rc

RESFILES = &
    def.res &
    help.res &
    rcdata.res &
    restyp.res &
    t1.res &
    t2.res &
    t3.res &
    test.res

.rc.res :
    $(RC) $(RCFLAGS) $<
    cmp $@ $*.rez

test : $(RESFILES) .symbolic
    @echo Test passed

clean : .symbolic
    rm -f *.res
